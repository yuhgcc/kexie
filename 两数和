class Solution {
public:
        int sum(int num1, int num2) {
        int answer;
        answer=num1+num2;
        return answer;
        }
    int main()
    {
        int a,b;
        cout<<"请输入两个整数："<<endl;
        cin>>a>>b;
        cout<<"这两个整数的和为："<<sum(a,b)<<endl;
        return 0;
    }
    
    
};
